{
    "id": "media",
    "name": "Media",
    "vendor": "se.buddhaflow.player.media.retro.plugins.music",
    "version": "0.0.1",
    "type": "plugin",
    "views": {
        "artist": {
            "regexp": "^bungalow:([a-zA-Z0-9@]+):artist:([a-zA-Z0-9]+)$"
        },
        "album": {
            "regexp": "^bungalow:([a-zA-Z0-9@]+):album:(.*)$"
        },
        "year": {
            "regexp": "^bungalow:([a-zA-Z0-9@]+):year:([0-9]+)$"
        },
        "playlists": {
            "regexp": "^bungalow:([a-zA-Z0-9@]+):library:playlists$"
        },
        "playlist": {
            "regexp": "^bungalow:@([a-zA-Z0-9@]+):history|(user:(.*):)?(playlist:(.*))|(library)|(artist:([a-zA-Z0-9]+):top:([0-9]+)))$"
        },
        "show": {
            "regexp": "^bungalow:([a-zA-Z0-9@]+):show:(.*)$"
        },
        "series": {
            "regexp": "^bungalow:([a-zA-Z0-9@]+):series:(.*)$"
        },
        "toplist": {
            "regexp": "^bungalow:([a-zA-Z0-9@]+):ainternal:toplist$"
        },
        "publisher": {
            "regexp": "^bungalow:([a-zA-Z0-9@]+):apublisher:(.*)$"
        },
        "episode": {
            "regexp": "^bungalow:([a-zA-Z0-9@]+):episode:(.*)$"
        },
        "chart": {
            "regexp": "^bungalow:([a-zA-Z0-9@]+):chart:(.*)$"
        },
        "playqueue": {
            "regexp": "^bungalow:([a-zA-Z0-9@]+):internal:playqueue$"
        },
        "category": {
            "regexp": "^bungalow:([a-zA-Z0-9@]+):category:(.*)$"
        },
        "country": {
            "regexp": "^bungalow:([a-zA-Z0-9@]+):country:(.*)$"
        },
        "audiobook": {
            "regexp": "^bungalow:([a-zA-Z0-9@]+):audiobook:(.*)$"
        },
        "author": {
            "regexp": "^bungalow:([a-zA-Z0-9@]+):author:([a-zA-Z0-9]+)$"
        },
        "search_new": {
            "regexp": "^bungalow:([a-zA-Z0-9@]+):label:([a-zA-Z0-9\\ ]+)$"
        },
        "meditation": {
            "regexp": "^bungalow:([a-zA-Z0-9@]+):meditation:(.*)$"
        },
        "search": {
            "regexp": "^bungalow:([a-zA-Z0-9@]+):search:(.*)$"
        },
        "user": {
            "regexp": "^bungalow:([a-zA-Z0-9@]+):user:(.*)$"
        }
    }
}
