{
    "id": "media",
    "name": "Media",
    "vendor": "se.buddhaflow.player.media.retro.plugins.music",
    "version": "0.0.1",
    "type": "plugin",
    "views": {
        "artist": {
            "regexp": "artist:([a-zA-Z0-9]+)$"
        },
        "album": {
            "regexp": "album:(.*)$"
        },
        "year": {
            "regexp": "year:([0-9]+)$"
        },
        "playlists": {
            "regexp": "ibrary:playlists$"
        },
        "playlist": {
            "regexp": "(history|(user:(.*):)?(playlist:(.*))|(library)|(artist:([a-zA-Z0-9]+):top:([0-9]+)))$"
        },
        "show": {
            "regexp": "show:(.*)$"
        },
        "series": {
            "regexp": "series:(.*)$"
        },
        "toplist": {
            "regexp": "internal:toplist$"
        },
        "publisher": {
            "regexp": "publisher:(.*)$"
        },
        "episode": {
            "regexp": "episode:(.*)$"
        },
        "chart": {
            "regexp": "chart:(.*)$"
        },
        "playqueue": {
            "regexp": "internal:playqueue$"
        },
        "category": {
            "regexp": "category:(.*)$"
        },
        "country": {
            "regexp": "country:(.*)$"
        },
        "audiobook": {
            "regexp": "audiobook:(.*)$"
        },
        "author": {
            "regexp": "author:([a-zA-Z0-9]+)$"
        },
        "search_new": {
            "regexp": "label:([a-zA-Z0-9\\ ]+)$"
        },
        "meditation": {
            "regexp": "meditation:(.*)$"
        },
        "search": {
            "regexp": "search:(.*)$"
        },
        "user": {
            "regexp": "user:(.*)$"
        }
    }
}
