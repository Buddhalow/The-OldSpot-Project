{
    "id": "bungalow",
    "name": "Bungalow core framework",
    "type": "plugin",
    "views": {
        "config": {
            "regexp": "config"
        },
        "service": {
            "regexp": "service"
        },
        "appviewstack": {
            "regexp": "app:(.*)"
        },
        "plugin": {
            "regexp": "plugin"
        },
        "start": {
            "regexp": "internal:start"
        },
        "search": {
            "regexp": "search:(.*)"
        },
        "hashtag": {
            "regexp": "hashtag:(.*)"
        },
        "nowplaying": {
            "hidesSidebar": true,
            "regexp": "internal:nowplaying"
        }
    }
}
