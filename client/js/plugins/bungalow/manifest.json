{
    "id": "bungalow",
    "name": "Bungalow core framework",
    "type": "plugin",
    "views": {
        "config": {
            "regexp": "^bungalow:config"
        },
        "service": {
            "regexp": "^bungalow:service"
        },
        "appviewstack": {
            "regexp": "^bungalow:app:(.*)"
        },
        "plugin": {
            "regexp": "^bungalow:plugin"
        },
        "concept": {
            "regexp": "^bungalow:concept"
        },
        "start": {
            "regexp": "^bungalow:internal:start"
        },
        "search": {
            "regexp": "^bungalow:search:(.*)"
        },
        "hashtag": {
            "regexp": "^bungalow:hashtag:(.*)"
        },
        "nowplaying": {
            "hidesSidebar": true,
            "regexp": "^bungalow:internal:nowplaying"
        }
    }
}
