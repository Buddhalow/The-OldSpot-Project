{
    "id": "spotify",
    "name": "Spotify",
    "vendor": "qi.spotify",
    "version": "0.0.1",
    "type": "plugin",
    "views": {
        "artist": {
            "regexp": "^spotify:artist:([a-zA-Z0-9]+)$"
        },
        "album": {
            "regexp": "^spotify:album:(.*)$"
        },
        "year": {
            "regexp": "^spotify:year:([0-9]+)$"
        },
        "playlists": {
            "regexp": "^spotify:me"
        },
        "playlist": {
            "regexp": "^spotify:(history|(user:(.*):)?(playlist:(.*))|(library)|(artist:([a-zA-Z0-9]+):top:([0-9]+)))$"
        },
        "show": {
            "regexp": "^spotify:show:(.*)$"
        },
        "series": {
            "regexp": "^spotify:series:(.*)$"
        },
        "toplist": {
            "regexp": "^spotify:internal:toplist$"
        },
        "publisher": {
            "regexp": "^spotify:publisher:(.*)$"
        },
        "episode": {
            "regexp": "^spotify:episode:(.*)$"
        },
        "chart": {
            "regexp": "^spotify:chart:(.*)$"
        },
        "playqueue": {
            "regexp": "^spotify:internal:playqueue$"
        },
        "category": {
            "regexp": "^spotify:category:(.*)$"
        },
        "country": {
            "regexp": "^spotify:country:(.*)$"
        },
        "audiobook": {
            "regexp": "^spotify:audiobook:(.*)$"
        },
        "author": {
            "regexp": "^spotify:author:([a-zA-Z0-9]+)$"
        },
        "search_new": {
            "regexp": "^spotify:label:([a-zA-Z0-9\\ ]+)$"
        },
        "meditation": {
            "regexp": "^spotify:meditation:(.*)$"
        },
        "search": {
            "regexp": "^^spotify:search:(.*)$"
        },
        "user": {
            "regexp": "^spotify:user:(.*)$"
        }
    }
}
